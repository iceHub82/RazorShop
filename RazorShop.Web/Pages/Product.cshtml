@inherits RazorSliceHttpResult<ProductVm>

<!DOCTYPE html>
<html lang="en">
<head>
    @(await RenderPartialAsync<Head>())
</head>
<body>
    @(await RenderPartialAsync<Navigation>())
    @(await RenderPartialAsync<Header>())
    @(await RenderPartialAsync<Product, ProductVm>(Model))
    @(await RenderPartialAsync<Footer>())

    <script src="/lib/jquery/dist/jquery.min.js"></script>
    <script src="/js/site.js"></script>
</body>
</html>